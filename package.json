{
  "name": "react-native-npm-template",
  "version": "0.0.1",
  "description": "Template to quickly create a React Native library",
  "author": {
    "name": "Hugo EXTRAT",
    "email": "extrat.h@gmail.com",
    "url": "https://www.github.com/huextrat"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/huextrat/react-native-npm-template"
  },
  "bugs": {
    "url": "https://github.com/huextrat/react-native-npm-template/issues"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "format": "prettier --write ./src",
    "format:check": "prettier --check --cache ./src",
    "prepare": "husky install",
    "build": "tsc",
    "release": "semantic-release"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "devDependencies": {
    "@commitlint/cli": "18.4.3",
    "@commitlint/config-conventional": "18.4.3",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@types/jest": "29.5.11",
    "@types/node": "20.10.4",
    "@types/react": "18.2.43",
    "@types/react-native": "0.72.8",
    "@typescript-eslint/eslint-plugin": "6.14.0",
    "@typescript-eslint/parser": "6.14.0",
    "eslint": "8.55.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-react": "7.33.2",
    "husky": "8.0.3",
    "prettier": "3.1.1",
    "react": "18.2.0",
    "react-native": "0.73.0",
    "semantic-release": "22.0.10",
    "typescript": "5.3.3"
  }
}
